// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider     = "mysql"
    url          = env("DATABASE_URL")
    relationMode = "prisma"
}

model Commune {
    codeInsee String  @id @unique
    name      String
    stats     Stats[]
}

model Stats {
    codeInsee String
    year      Int
    commune   Commune @relation(fields: [codeInsee], references: [codeInsee], onDelete: Cascade)
    name      String
    category  String
    value     Int

    @@unique([codeInsee, year, category, name])
}
